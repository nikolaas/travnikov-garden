@font-face {
  font-family: 'Open Sans Bold';
  font-weight: 700;
  src:  url('../resources/fonts/opensans-bold.eot');
  src:  url('../resources/fonts/opensans-bold.eot?#iefix') format('embedded-opentype'),
        url('../resources/fonts/opensans-bold.woff2') format('woff2'),
        url('../resources/fonts/opensans-bold.woff') format('woff'),
        url('../resources/fonts/opensans-bold.svg#open_sansbold') format('svg');
}

